{
  "version": 2,
  "builds": [
    {
      "src": "api/contact.js",
      "use": "@vercel/node",
      "config": {
        "includeFiles": ["api/**/*.js"]
      }
    },
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/contact"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}