{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": { "distDir": "build" }
    },
    {
      "src": "api/contact.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/contact",
      "methods": ["POST"],
      "dest": "/api/contact.js"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}